  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.2",
    "@types/nunjucks": "^3.2.3",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
