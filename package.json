    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "pa11y-ci": "^3.0.1",
    "selenium-webdriver": "^4.9.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6"
