  "main": "app.js",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "build": "npx tsc && cp -R views dist && cp -R public dist",
    "hotReloadDev": "npx nodemon ./app.ts",
    "lint": "npx eslint . --ext .ts && echo \"\n### Linting Successful - No standards violations detected! ###\n\"",
    "lintFix": "npx eslint . --ext .ts --fix",
    "test": "mocha 'test/unit/*.ts' --require ts-node/register",
    "test-ui": "mocha --timeout 10000 test/ui/**/*.js",
    "prepare": "husky install"
  },
